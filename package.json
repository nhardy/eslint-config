{
  "name": "eslint-config-nhardy",
  "version": "0.0.3",
  "description": "An mi9-esque ESLint config which extends eslint-config-airbnb",
  "main": "dist/index.js",
  "scripts": {
    "build": "./node_modules/.bin/babel ./src/index.js -o ./dist/index.js",
    "preversion": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "./node_modules/.bin/babel ./src/index.js --watch -o ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhardy/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "config",
    "nhardy",
    "mi9"
  ],
  "author": "Nathan Hardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhardy/eslint-config/issues"
  },
  "homepage": "https://github.com/nhardy/eslint-config#readme",
  "dependencies": {
    "babel-eslint": "^6.0.0",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-react": "^4.2.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "eslint": "^2.4.0"
  }
}
