{
  "name": "eslint-config-nhardy",
  "version": "0.2.1",
  "description": "An mi9-esque ESLint config which extends eslint-config-airbnb",
  "main": "index.js",
  "scripts": {
    "build": "babel ./src --source-maps --out-dir ./dist",
    "clean": "GLOBIGNORE=.gitignore:.npmignore rm -rf ./dist/*",
    "preversion": "npm run build",
    "postversion": "git push --follow-tags",
    "start": "npm run clean && npm run build",
    "test": "eslint --fix ./src",
    "watch": "npm run clean && babel ./src --source-maps --watch --out-dir ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhardy/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "config",
    "nhardy",
    "mi9"
  ],
  "author": "Nathan Hardy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nhardy/eslint-config/issues"
  },
  "homepage": "https://github.com/nhardy/eslint-config#readme",
  "dependencies": {
    "babel-eslint": "^6.1.0",
    "eslint-config-airbnb": "^11.1.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.13.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-mocha": "^4.5.1",
    "eslint-plugin-react": "^6.3.0",
    "lodash": "^4.16.1"
  },
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.6.0"
  },
  "peerDependencies": {
    "eslint": "^3.6.0",
    "eslint-plugin-babel": "^3.3.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-mocha": "^4.5.1",
    "eslint-plugin-react": "^6.3.0"
  }
}
